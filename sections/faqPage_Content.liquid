
<div class="faq_page_container">

  <div class="faq__wrapper">
  <div class="faq_page_content">
          {% for block in section.blocks %}
            <div class="page_container" id={{ block.settings.heading_ID  | downcase | replace: ' ', '-'  }}>
                 <div class="content_heading"> {{ block.settings.heading }}</div>
                 <div class="conte_descp"> {{ block.settings.descp }}</div>
            </div>
            
          {% endfor %}
  </div>
    </div>
  
</div>





{% schema %}
  {
    "name": "FAQ Page Content",
    "settings": [],
    "blocks": [
      {
        "name":"Text Content",
        "type":"block",
        "settings" : [
           {
            "type": "text",
            "id": "heading_ID",
            "label": "Heading ID"
          },
          {
            "type": "text",
            "id": "heading",
            "label": "Heading"
          },
          {
            "type": "richtext",
            "id": "descp",
            "label": "Content"
          }
          
        ]
      }
    ],
      "presets":[
      {
        "name":"FAQ Page Content"
      }
    ]
  }
{% endschema %}